<h1>Listing djs</h1>

<table>
  <thead>
    <tr>
      <th>Name first</th>
      <th>Name last</th>
      <th>Email</th>
      <th>Timezone</th>
      <th>Requested slot</th>
      <th>Live</th>
      <th>Skype handle</th>
      <th>Google handle</th>
      <th>Irc handle</th>
      <th>Genres</th>
      <th>Frequency</th>
      <th>Cc license accepted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @djs.each do |dj| %>
      <tr>
        <td><%= dj.name_first %></td>
        <td><%= dj.name_last %></td>
        <td><%= dj.email %></td>
        <td><%= dj.timezone %></td>
        <td><%= dj.requested_slot %></td>
        <td><%= dj.live %></td>
        <td><%= dj.skype_handle %></td>
        <td><%= dj.google_handle %></td>
        <td><%= dj.irc_handle %></td>
        <td><%= dj.genres %></td>
        <td><%= dj.frequency %></td>
        <td><%= dj.cc_license_accepted %></td>
        <td><%= link_to 'Show', dj %></td>
        <td><%= link_to 'Edit', edit_dj_path(dj) %></td>
        <td><%= link_to 'Destroy', dj, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dj', new_dj_path %>
