          <div class="row">
            <div class="span2">
              <div class="box1"></div>
            </div>
            <div class="span10">
              <div class="box0">
              <%= render "shared/top_nav" %>
              <%= render "shared/side_nav" %>
              <%= yield %>
              <%= render "shared/footer" %>
